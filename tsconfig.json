{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "target": "es2020",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": true,
    //"declaration": true,
    //"declarationMap": true,
    //"sourceMap": true,
    "removeComments": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "rootDirs": ["src"],
    "baseUrl": "src"
  },
  "include": ["src"],
  "exclude": []
}
